{
    "name": "psi/description",
    "description": "Allow third-party libraries to add descriptions to an object instance",
    "homepage": "http://cmf.symfony.com",
    "license": "MIT",
    "authors": [
        {
            "name": "PSI Community",
            "homepage": "https://psiphp.github.io"
        }
    ],
    "prefer-stable": true,
    "require": {
        "php": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.5",
        "phpbench/phpbench": "~0.12.0"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Psi\\Component\\Description\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Psi\\Component\\Description\\Tests\\": "tests/",
            "Psi\\Component\\Description\\Benchmarks\\": "benchmarks/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
